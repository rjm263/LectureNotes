% Theorem etc numbering, aliascing and referencing
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
\theoremstyle{plain}
\newtheorem{thm}{Theorem}[section]
\newaliascnt{lemma}{thm}
\newtheorem{lemma}[lemma]{Lemma}
\aliascntresetthe{lemma}
\providecommand*{\lemmaautorefname}{Lemma}
\newaliascnt{prop}{thm}
\newtheorem{prop}[prop]{Proposition}
\aliascntresetthe{prop}
\providecommand*{\propautorefname}{Proposition}
\newaliascnt{cor}{thm}
\newtheorem{cor}[cor]{Corollary}
\aliascntresetthe{cor}
\providecommand*{\corautorefname}{Corollary}

\theoremstyle{definition}
\newaliascnt{defi}{thm}
\newtheorem{defi}[defi]{Definition}
\aliascntresetthe{defi}
\providecommand*{\defiautorefname}{Definition}
\newaliascnt{eg}{thm}
\newtheorem{eg}[eg]{Example}
\aliascntresetthe{eg}
\providecommand*{\egautorefname}{Example}
%\newtheorem{ex}thm{Exercise}
\newaliascnt{remark}{thm}
\newtheorem{remark}[remark]{Remark}
\aliascntresetthe{remark}
\providecommand*{\remarkautorefname}{Remark}
\newaliascnt{pro}{thm}
\newtheorem{pro}[pro]{Properties}
\aliascntresetthe{pro}
\providecommand*{\proautorefname}{Properties}

% remove last dot in numbering
\AtBeginDocument{\xpatchcmd{\@thm}{\thm@headpunct{.}}{\thm@headpunct{}}{}{}}

\renewcommand{\qedsymbol}{$\square$}
\newcommand{\thmautorefname}{Theorem}

